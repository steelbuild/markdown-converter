<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>markdown_to_html</RootNamespace>
    <PackageTags>steelcon</PackageTags>
    <RepositoryUrl>https://github.com/steelbuild/markdown-converter</RepositoryUrl>
    <Description>Converts MarkDown into HTML.  (Please don't use in production!)</Description>
    <PackageReleaseNotes>Contains performance and functionality fixes,  strongly recommend that you upgrade.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Markdig" Version="0.17.0" />
  </ItemGroup>

</Project>
